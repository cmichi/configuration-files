[user]
	name = cmichi
	email = mich@elmueller.net
[merge]
	tool = vimdiff
[core]
	editor = vi
	excludesfile = /home/michi/.gitignore
[color]
	diff = true
	ui = true
[alias]
	unstage = rm --cached  
	ca = commit -a -m
	ci = commit 
	st = status
	lg = log --pretty=format:'%h - %an, %ar : %s'
[push]
	default = tracking
